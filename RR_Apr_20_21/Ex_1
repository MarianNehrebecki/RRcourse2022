for i in range(1, 100 + 1):  # i -> 1, 2, ..., 100
    divisible_by_3 = i % 3 == 0
    divisible_by_5 = i % 5 == 0
    if divisible_by_3:
        print('Fizz', end='')  # without displaying new line since there can be Buzz here
    if divisible_by_5:
        print('Buzz', end='')
    if not divisible_by_3 and not divisible_by_5:
        print(i, end='')
    print()
